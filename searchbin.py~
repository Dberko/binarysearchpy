def search_binary(xs, target):
    lb = 0
    ub = len(xs)
    
    while True:
        if lb == ub:
            return -1
        mid = (lb + up) // 2
        miditem = xs[mid]
        
        if miditem == target:
            return mid
        if miditem < target:
            lb = mid + 1
        else:
            ub = mid

